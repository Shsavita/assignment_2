import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
		 Scanner sc=new  Scanner(System.in);
		int n=sc.nextInt();
		int[] a=new int[n];
		for(int i=0;i<n;i++)
		{
		a[i]=sc.nextInt();
		}
		int t=sc.nextInt();
		int f=0;
		for(int i=0;i<n;i++)
		{
			if(a[i]==t){
			System.out.println(i);
			f=1;
			}
		}
		if(f==0)
		System.out.println("-1");
    }
}
